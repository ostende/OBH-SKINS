#!/bin/sh

rm -rf /usr/share/enigma2/MX_Sline-Black
rm -rf /usr/uninstall/MX_Sline-Black.nab

activeskin=`grep -c -e 'MX_Sline-Black' /etc/enigma2/settings`

if test $activeskin = 1
then
sed -e '/MX_Sline-Black/d' /etc/enigma2/settings > /tmp/settings
mv /tmp/settings /etc/enigma2/settings
killall -9 enigma2
fi

exit 0
